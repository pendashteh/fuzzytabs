{
  "manifest_version": 3,
  "name": "Quick Tab Finder",
  "version": "1.0",
  "description": "Fuzzy search to quickly find and switch tabs",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      }
    }
  }
}
