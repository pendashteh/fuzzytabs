{
  "manifest_version": 3,
  "name": "Fuzzy Tabs",
  "version": "1.0.0",
  "description": "Fuzzy search to quickly find and switch between tabs",
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Quick Tab Finder"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      },
      "description": "Open tab finder"
    }
  }
}
